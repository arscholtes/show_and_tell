<% flash.each do |key, value| %>
  <div>
    <%= value %>
  </div>
<% end %>

<% if user_signed_in? %>
  <p>Signed in as <%= current_user.name %></p>
<% end %>
<ul>
  <li><%= button_to 'Home', root_path %></li>
  <li><%= button_to 'Users', users_path %></li>
  <li><%= button_to 'logout', destroy_user_session_path, method: :delete  %></li>
</ul>
<div>
  <%= line_chart User.group_by_minute(:created_at).count  %>
</div>
